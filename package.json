{
  "name": "storybook-addon-angular-router",
  "version": "0.0.0",
  "description": "A simple plugin to make working with the angular router in storybook easier ✨",
  "keywords": [
    "Storybook",
    "Angular Router",
    "Mock",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bastiW/storybook-addon-angular-router"
  },
  "type": "module",
  "license": "MIT",
  "author": "Sebastian Weiß",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "storybook": "ng run storybook-addon-angular-router:storybook",
    "build-storybook": "ng run storybook-addon-angular-router:build-storybook"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.0",
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/angular": "^9.1.3",
    "@storybook/cli": "^9.1.3",
    "@storybook/react-vite": "^9.1.0",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "boxen": "^7.1.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "~7.8.0",
    "storybook": "^9.1.0",
    "ts-dedent": "^2.2.0",
    "tslib": "^2.3.0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "vite": "^5.3.5",
    "zone.js": "~0.15.0",
    "zx": "^8.4.1"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "Angular",
      "Storybook"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
